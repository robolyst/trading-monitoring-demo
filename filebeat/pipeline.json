{
    "description": "Parses the timestamp field",
    "processors": [
        {
            "date": {
                "field": "json.timestamp",
                "target_field": "@timestamp",
                "formats": [
                    "ISO8601"
                ],
                "timezone": "UTC"
            }
        }
    ],
    "on_failure": [
        {
            "set": {
                "field": "_ingest_error",
                "value": "{{_ingest.on_failure_message}}"
            }
        }
    ]
}